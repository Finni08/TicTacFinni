<Window x:Class="TikTacFinni.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TikTacFinni"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" ResizeMode="CanMinimize">
    <Canvas>
        <Button x:Name="Singelplayerbtn" Content="Singelplayer" HorizontalAlignment="Left" VerticalAlignment="Top" Height="70" Width="200" Click="Singelplayerbtn_Click" Canvas.Left="100" Canvas.Top="182"/>
        <Button x:Name="Multiplayerbtn" Content="Multiplayer" HorizontalAlignment="Left" VerticalAlignment="Top" Height="70" Width="200" Click="Multiplayerbtn_Click" Canvas.Left="500" Canvas.Top="182"/>

    </Canvas>
</Window>
