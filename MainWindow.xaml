        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TikTacFinni"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="Singelplayerbtn" Content="Singelplayer" HorizontalAlignment="Left" Margin="100,0,0,0" VerticalAlignment="Center" Height="70" Width="200" RenderTransformOrigin="0.527,0.468" Click="Singelplayerbtn_Click"/>
        <Button x:Name="Multiplayerbtn" Content="Multiplayer" HorizontalAlignment="Left" Margin="500,0,100,0" VerticalAlignment="Center" Height="70" Width="200" Click="Multiplayerbtn_Click"/>

    </Grid>
</Window>
